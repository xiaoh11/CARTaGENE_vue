{
  "name": "vapp",
  "version": "0.12.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build_staging": "NODE_ENV=production vue-cli-service build --mode staging",
    "make_tar": "scripts/make_dist_tar.sh $npm_package_version prod",
    "make_tar_staging": "scripts/make_dist_tar.sh $npm_package_version staging",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "dbg": "webpack -h"
  },
  "dependencies": {
    "@coreui/vue": "^4.4.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/vue-fontawesome": "^3.0.0-4",
    "@vue/cli": "^5.0.0-rc.0",
    "axios": "^0.23.0",
    "bootstrap": "^4.6.2",
    "colors": "1.4.0",
    "core-js": "^3.6.5",
    "d3": "^7.1.1",
    "devbridge-autocomplete": "^1.4.11",
    "express": "^4.18.2",
    "igv": "^2.10.4",
    "just-clone": "^3.2.1",
    "locuszoom": "^0.14.0",
    "sinon": "^14.0.2",
    "tabulator-tables": "^4.9.3",
    "vue": "^3.2.22",
    "vue-gtag": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-unit-mocha": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/compiler-sfc": "^3.2.22",
    "@vue/test-utils": "^2.0.0-0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.20.0",
    "eslint-plugin-vue": "^8.0.3",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.14",
    "webpack-cli": "^4.9.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      },
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      },
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      },
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
